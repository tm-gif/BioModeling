   "metadata": {},
   "outputs": [],
   "source": [
    "nsteps =50\n",
    "x = .67\n",
    "y = 0\n",
    "fig = plt.figure(figsize=(7,7))\n",
    "\n",
    "for r in np.arange(2.8, 4, .001):\n",
    "    y += 1\n",
    "    for i in range(nsteps + y%50):\n",
    "        x = r*x*(1-x)\n",
    "        plt.plot(x, i, 'k.', markersize = 1)"
